Cell["\<\
We created a few functions in order to Import all of the Image files, and \
export their Pixel Data to a Text file for computation in our main program.\
\>", "Text",
 CellChangeTimes->{{3.6403709234623685`*^9, 3.640370960493634*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"(*", 
   RowBox[{
   "Export", " ", "the", " ", "given", " ", "file", " ", "as", " ", "a", " ", 
    "text", " ", "file", " ", "containing", " ", "the", " ", "data", " ", 
    "for", " ", "each", " ", "individual", " ", "Pixel"}], "*)"}], 
  "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{
     RowBox[{"export", "[", "file_", "]"}], ":=", 
     RowBox[{"Block", "[", 
      RowBox[{
       RowBox[{"{", "}"}], ",", "\[IndentingNewLine]", 
       RowBox[{"(*", 
        RowBox[{
        "Set", " ", "the", " ", "Path", " ", "names", " ", "for", " ", "the", 
         " ", "file"}], "*)"}], "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"imagefile", "=", 
         RowBox[{
          RowBox[{"NotebookDirectory", "[", "]"}], "<>", "\"\<yalefaces/\>\"",
           "<>", "file"}]}], ";", "\[IndentingNewLine]", 
        RowBox[{"textfile", "=", 
         RowBox[{
          RowBox[{"NotebookDirectory", "[", "]"}], "<>", "\"\<facetext/\>\"", 
          "<>", 
          RowBox[{"FileBaseName", "[", "file", "]"}], "<>", 
          "\"\<.txt\>\""}]}], ";", "\[IndentingNewLine]", 
        "\[IndentingNewLine]", 
        RowBox[{"(*", 
         RowBox[{"Import", " ", "the", " ", "Image", " ", "File"}], "*)"}], 
        "\[IndentingNewLine]", 
        RowBox[{"image", "=", 
         RowBox[{"Import", "[", "imagefile", "]"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"data", "=", 
         RowBox[{"Map", "[", 
          RowBox[{"Last", ",", 
           RowBox[{"ImageData", "[", "image", "]"}], ",", 
           RowBox[{"{", "2", "}"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
        RowBox[{"(*", 
         RowBox[{
         "Export", " ", "in", " ", "Table", " ", "format", " ", "the", " ", 
          "Image", " ", "Files", " ", "Pixel", " ", "Data"}], "*)"}], 
        "\[IndentingNewLine]", 
        RowBox[{"Export", "[", 
         RowBox[{"textfile", ",", "data", ",", "\"\<Table\>\""}], "]"}], 
        ";"}]}], "\[IndentingNewLine]", "]"}]}], ";"}], "\[IndentingNewLine]",
    "\[IndentingNewLine]", 
   RowBox[{"(*", 
    RowBox[{
    "Get", " ", "a", " ", "list", " ", "of", " ", "all", " ", "the", " ", 
     "Image", " ", "Files", " ", "in", " ", "the", " ", "Directory"}], "*)"}],
    "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"files", " ", "=", " ", 
     RowBox[{"Map", "[", 
      RowBox[{"FileNameSplit", ",", " ", 
       RowBox[{"FileNames", "[", 
        RowBox[{
         RowBox[{"NotebookDirectory", "[", "]"}], "<>", 
         "\"\<yalefaces/*.gif\>\""}], "]"}]}], "]"}]}], ";"}], 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"files", " ", "=", 
     RowBox[{"Map", "[", 
      RowBox[{"Last", ",", " ", "files"}], "]"}]}], ";"}], 
   "\[IndentingNewLine]", 
   RowBox[{"(*", 
    RowBox[{
    "Convert", " ", "all", " ", "Image", " ", "Files", " ", "into", " ", 
     "Text", " ", "files", " ", "with", " ", "Pixel", " ", "Data"}], "*)"}], 
   "\[IndentingNewLine]", 
   RowBox[{"Map", "[", 
    RowBox[{"export", ",", "files"}], "]"}], "\[IndentingNewLine]", 
   "\[IndentingNewLine]"}]}]], "Input",
 CellChangeTimes->{{3.640284353093482*^9, 3.6402843755663843`*^9}, {
